    $(document).ready(function(){
  $('#logout').on('click', function (e) {
        e.preventDefault();
        sessionStorage.userId = '';
        window.location.href = '/logout'
    })
         if (sessionStorage.userId) {
     $('#login').hide();
     $('#join').hide();
     $('#logout').show();
    }
